{
  "name": "socket",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "rm": "node ace migration:run",
    "mm": "node ace make:migration",
    "ms": "node ace seeders:run",
    "rs": "node ace db:seed",
    "mc": "node ace make:controller",
    "mv": "node ace make:validator",
    "mmm": "node ace make:model",
    "mrb": "node ace migration:rollback"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.0",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "@types/qrcode": "^1.5.1",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/attachment-lite": "^1.0.7",
    "@adonisjs/auth": "^8.2.1",
    "@adonisjs/core": "^5.8.4",
    "@adonisjs/i18n": "^1.5.6",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/mail": "^8.1.2",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/view": "^6.1.5",
    "@rocketseat/adonis-bull": "^1.0.4",
    "@types/socket.io": "^3.0.2",
    "adonisjs-cloudinary": "^0.1.6",
    "crypto-js": "^4.1.1",
    "csvtojson": "^2.0.10",
    "luxon": "^2.4.0",
    "moment": "^2.29.4",
    "moment-range": "^4.0.2",
    "mysql": "^2.18.1",
    "phc-argon2": "^1.1.3",
    "proxy-addr": "^2.0.7",
    "qrcode": "^1.5.3",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rss-to-json": "^2.1.1",
    "socket.io": "^4.5.1",
    "source-map-support": "^0.5.21"
  }
}
